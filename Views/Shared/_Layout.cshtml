<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>["Title"] - CampusLink</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>
<nav class="navbar navbar-expand-lg navbar-dark bg-success mb-4">
    <div class="container">
        <a class="navbar-brand" asp-controller="Students" asp-action="Index">CampusLink</a>
        
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ms-auto">
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Students" asp-action="Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Students" asp-action="Register">Register Student</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-controller="Students" asp-action="List">View Student List</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<body>
    
    <div class="container py-4">
        <h1 class="text-primary text-center mb-4">CampusLink Student Registration</h1>
        @RenderBody()
       
    </div>
</body>
</html>
